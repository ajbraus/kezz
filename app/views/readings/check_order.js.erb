<% if @ids[0].any? %>
	<% @ids[0].each do |oooi| %>
		$("#<%= oooi %>").children('u').effect("highlight", { color:"red" }, 10000);
	<% end %>
<% end %>

<% if @ids[1].any? %>
	<% @ids[1].each do |ioid| %>
		$("#<%= ioid %>").children('u').effect("highlight", { color:"green" }, 10000);
		//select by name and add background color if right or wrong
	<% end %>
<% end %>
